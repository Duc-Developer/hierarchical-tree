{
  "name": "hierarchical-tree",
  "version": "1.0.0",
  "description": "This is library supported for convert array to hierarchical tree with move, swap, delete,... node faster",
  "main": "./dist/index.esm.js",
  "module": "./dist/index.esm.js",
  "types": "dist/index.d.ts",
  "author": "Duc-Developer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Duc-Developer/hierarchical-tree.git"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "bun-types": "latest",
    "dts-bundle-generator": "^9.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0"
  },
  "engines": {
    "bun": ">=1.0.15"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "scripts": {
    "test": "bun test",
    "postinstall": "husky install && bun run pre-husky",
    "pre-husky": "npx husky add .husky/pre-commit 'bun lint-staged'",
    "check": "prettier --check 'src/**/*'",
    "format": "prettier --write 'src/**/*'",
    "release": "bun bundle && changeset publish --provenance --access public",
    "bundle": "rm -rf dist/ && bun run scripts/bundle.js && bun run bundle:declaration",
    "bundle:declaration": "dts-bundle-generator --config dts-bundle-config.json",
    "cleanup": "git branch | grep -v 'master$' | grep -v 'develop$' | xargs git branch -D"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "bun run format",
      "git add ."
    ]
  },
  "bugs": {
    "url": "https://github.com/Duc-Developer/hierarchical-tree/issues"
  },
  "homepage": "https://github.com/Duc-Developer/hierarchical-tree#readme",
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "dist"
  ],
  "keywords": [
    "tree",
    "hierachical-tree",
    "antd",
    "antd",
    "tree",
    "array",
    "to",
    "tree",
    "list",
    "to",
    "tree",
    "array-helper",
    "child",
    "parent",
    "datastructures",
    "navigation",
    "descendants",
    "ancestors",
    "util",
    "tree",
    "util"
  ]
}
